<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE root[
        <!ELEMENT root ANY>
        <!ELEMENT resultMap ANY>
        <!ATTLIST resultMap
                id ID #REQUIRED
                >
        <!ELEMENT result ANY>
        <!ATTLIST result
                property CDATA #REQUIRED
                column CDATA #REQUIRED
                jdbcType CDATA #REQUIRED>
        <!ELEMENT id ANY>
        <!ATTLIST id
                property CDATA #REQUIRED
                column CDATA #REQUIRED
                jdbcType CDATA #REQUIRED>
        ]>
<root>
    <resultMap id="pojo.Question">
        <id property="id" column="id" jdbcType="BIGINT"/>
